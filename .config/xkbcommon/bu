    key <AB03> { [      	   c,          C,       ccedilla,              Ccedilla ] }; // c C ç Ç
    key <AB04> { [      	   v,          V,      cabovedot,        	  Cabovedot ] }; // v V ċ Ċ
    key <AB05> { [      	   b,          B,            eth,                	ETH ] }; // b B ð Ð
    key <AB06> { [      	   n,          N,     udiaeresis,            Udiaeresis ] }; // n N ü Ü
    key <AB07> { [      	   m,          M,     idiaeresis,         	 Idiaeresis ] }; // m M ï Ï
    key <AB08> { [  	   comma,       less,   dead_cedilla,    dead_doublecedilla ] }; // , < ¸ 
    key <AB09> { [ 		  period,    greater,  dead_abovedot,      	  dead_belowdot ] }; // . > ˙ ̣ //0x00B7
    key <AB10> { [		   slash,   question,   questiondown                   		] }; // / ? ¿
    
    key <AC01> { [      	   a,          A,         agrave,           	 Agrave ] }; // a A à À
    key <AC02> { [      	   s,          S,         ssharp            	    	] }; // s S ẞ ß
