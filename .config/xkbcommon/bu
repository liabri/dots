    key <AC04> { [      	   f,          F,     ediaeresis,       	 Ediaeresis ] }; // f F ë Ë //U201e U201c
    key <AC05> { [      	   g,          G,      gabovedot,        	  Gabovedot ] }; // g G ġ Ġ
    key <AC06> { [             h,          H,        hstroke,          	    Hstroke ] }; // h H ħ Ħ 
    key <AC07> { [      	   j,          J,    ucircumflex,        	Ucircumflex ] }; // j J û Û
