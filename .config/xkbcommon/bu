    key <AD06> { [             y,          Y,     ydiaeresis,            Ydiaeresis ] }; // y Y ÿ Ÿ
    key <AD07> { [             u,          U,         ugrave,                Ugrave ] }; // u U ù Ù
    key <AD08> { [             i,          I,    	  igrave,                igrave ] }; // i I ï Ï
    key <AD09> { [             o,          O,         ograve,                Ograve ] }; // o O ò Ò
    key <AD10> { [             p,          P,             oe,                    OE ] }; // p P œ Œ
    key <AD11> { [   bracketleft,  braceleft,  guillemotleft,   leftdoublequotemark ] }; // [ { « “
    key <AD12> { [  bracketright, braceright, guillemotright,  rightdoublequotemark ] }; // ] } » ”

    key <AE01> { [        	   1,	  exclam,     exclamdown				       	] }; // 1 ! ¡
