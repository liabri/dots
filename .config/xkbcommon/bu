    key <AC09> { [      	   l,          L,    ocircumflex,           Ocircumflex ] }; // l L ô Ô
    key <AC10> { [	   semicolon,      colon,     dead_grave, 	   dead_doublegrave	] }; // ; : ` 
    key <AC11> { [	  apostrophe,   quotedbl,     dead_acute, 	   dead_doubleacute ] }; // ' " ´ ˝

    key <AD01> { [             q,          Q,         aacute,                Aacute ] }; // q Q á Á
    key <AD02> { [             w,          W,             ae,                    AE ] }; // w W æ Æ
    key <AD03> { [             e,          E,         egrave,                Egrave ] }; // e E è È
    key <AD04> { [             r,          R,         eacute,                Eacute ] }; // r R é É
